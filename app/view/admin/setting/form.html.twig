{% extends 'admin/layout/index.html.twig' %}
{% block content %}
<div class="page-header">
	<h1>
		{{control}}
	</h1>
</div>

<div class="row-setting">
	<div class="col-xs-12">
		<div>
			<div>
			<div class="notification">
			{% for message in app.session.getFlashBag.get('message') %}
  			<script>alert('{{ message }}')</script>
  			{% endfor %}</div>
			</div>
			{% for s in social %}
			<form class="form-horizontal" role="form" action="{{ url('setting') }}" name="frmSetting" method="post" enctype="multipart/form-data">
				<div class="form-group">
					<label class="col-sm-2 control-label no-padding-right">Page Name:</label>
					<div class="col-sm-9">
						<input name="page_name" id="page_name" value="{{s.page_name}}" class="form-control" type="text">
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-2 control-label no-padding-right">Keywords:</label>
					<div class="col-sm-9">
						<input name="keywords" id="keywords" value="{{s.keywords}}" class="form-control" type="text">
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-2 control-label no-padding-right">Description:</label>
					<div class="col-sm-9">
						<input name="description" id="description" value="{{s.description}}" class="form-control" type="text">
					</div>
				</div>
				<div class="clearfix form-actions">
					<div class="col-md-offset-3 col-md-9">
						<input type="hidden" name="update" value="update" />
						<button onclick="return btnRegistry_onclick();" class="btn btn-info" type="submit">
							<i class="ace-icon fa fa-check bigger-110"></i>
							Submit
						</button>
						&nbsp; &nbsp; &nbsp;
						<button class="btn" type="reset">
							<i class="ace-icon fa fa-undo bigger-110"></i>
							Reset
						</button>
					</div>
				</div>
			</form>
		</div>
		{% endfor %}
	</div><!-- /.col -->
</div>
<script type="text/javascript">
function btnRegistry_onclick(){
	if(test_empty($('#page_name').val())){
		alert('Vui l√≤ng page name');document.frmSetting.focus();return false;
	}
	return true;
}
</script>

{% endblock %}
